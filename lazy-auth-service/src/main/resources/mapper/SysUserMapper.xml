<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuwj.auth.mapper.SysUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhuwj.auth.entity.SysUser">
    <result column="id" property="id" />
    <result column="gmt_create" property="gmtCreate" />
    <result column="create_id" property="createId" />
    <result column="gmt_modified" property="gmtModified" />
    <result column="modified_id" property="modifiedId" />
    <result column="is_deleted" property="isDeleted" />
    <result column="version" property="version" />
        <result column="username" property="username" />
        <result column="password" property="password" />
        <result column="nick_name" property="nickName" />
        <result column="avatar" property="avatar" />
        <result column="phone" property="phone" />
        <result column="latest_time" property="latestTime" />
        <result column="is_enabled" property="enabled" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        gmt_create,
        create_id,
        gmt_modified,
        modified_id,
        is_deleted,
        version,
        username, password, nick_name, avatar, phone, latest_time, is_enabled
    </sql>


</mapper>
